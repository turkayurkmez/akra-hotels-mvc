@model IEnumerable<Category>

<div class="list-group">
   <a asp-controller="Home" asp-action="Index" asp-route-page="1" asp-route-category="">Tüm kategoriler</a>
   @foreach (var category in Model)
    {
        <a  asp-controller="Home" asp-action="Index" asp-route-category="@category.Id" asp-route-page="1"  class="list-group-item list-group-item-action">@category.Name</a>
    }  
   
</div>